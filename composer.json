{
  "name": "drupal/omnipedia_access",
  "description": "Omnipedia - Access",
  "type": "drupal-custom-module",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Ambient.Impact",
      "homepage": "https://ambientimpact.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "drupal/hook_event_dispatcher": "^2.0 || ^3.0",
    "drupal/permissions_by_term": "^2.34 || ^3.1"
  },
  "extra": {
    "patches": {
      "drupal/permissions_by_term": {
        "Respect the Require all terms granted and Permission mode settings in Permission by entity module: https://www.drupal.org/project/permissions_by_term/issues/3072171#comment-14209554": "https://raw.githubusercontent.com/neurocracy/drupal-omnipedia-access/3.x/patches/drupal/permissions_by_term/merge-request-8-respect-require-all-terms-permission-by-entity.patch?token=GHSAT0AAAAAABVEVRT3N32T2GHFEI7OL7GMYU5HAKQ",
        "Permissions by Entity PHP notice for Commerce Product entities: https://www.drupal.org/project/permissions_by_term/issues/3231188#comment-14209558": "https://raw.githubusercontent.com/neurocracy/drupal-omnipedia-access/3.x/patches/drupal/permissions_by_term/merge-request-9-commerce-product-notices.patch?token=GHSAT0AAAAAABVEVRT2QXOBYSI4FZY54SBAYU5HAVQ",
        "PbE: Add entityTypeId to cache tags: https://www.drupal.org/project/permissions_by_term/issues/3238759#comment-14234566": "https://raw.githubusercontent.com/neurocracy/drupal-omnipedia-access/3.x/patches/drupal/permissions_by_term/3238759-PbE_respect_entity_type_id-4.patch?token=GHSAT0AAAAAABVEVRT3NWSMLJJDMYY5EDP4YU5HBWQ",
        "Node Access Rebuilt unnecessarily if no change to user terms: https://www.drupal.org/project/permissions_by_term/issues/3170389#comment-14242763": "https://raw.githubusercontent.com/neurocracy/drupal-omnipedia-access/3.x/patches/drupal/permissions_by_term/Issue-3170389-4-Node-Access-Rebuilt-unnecessarily.patch?token=GHSAT0AAAAAABVEVRT33PUB7PH5UQ7ZBOIEYU5HBEA"
      }
    }
  }
}
